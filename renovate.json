{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePatterns": [
        "goharbor/*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "docker harbor non-major dependencies",
      "groupSlug": "docker-harbor-minor-patch"
    }
  ]
}